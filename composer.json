{
  "name": "phapi/middleware-file-handler",
  "description": "Middleware responsible for handling files received from and sent to the client",
  "keywords": [
    "middleware", "file", "upload", "phapi"
  ],
  "version": "1.0.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "homepage": "http://phapi.github.io",
  "authors": [
    {
      "name": "Peter Ahinko",
      "email": "peter@ahinko.se",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "league/flysystem": "1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.10",
    "mockery/mockery": "0.9.*",
    "squizlabs/php_codesniffer": "2.*",
  },
  "autoload": {
    "psr-4": {
      "Phapi\\Endpoint\\": "src/Phapi/Endpoint/",
      "Phapi\\Middleware\\FileHandler\\": "src/Phapi/Middleware/FileHandler/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phapi\\Tests\\Middleware\\FileHandler\\": "tests/Phapi/Middleware/FileHandler/"
    }
  }
}
